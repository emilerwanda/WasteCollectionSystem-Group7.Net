<?xml version="1.0" encoding="utf-8"?>
<configuration>
	<packageSources>
		<add key="nuget.org" value="https://api.nuget.org/v3/index.json" />
	</packageSources>
	<packageSourceMapping>
		<packageSource key="nuget.org">
			<package pattern="*" />
			<package pattern="Microsoft.*" />
			<package pattern="System.*" />
			<package pattern="Azure.*" />
			<package pattern="Humanizer.*" />
			<package pattern="Newtonsoft.*" />
			<package pattern="Mono.*" />
			<package pattern="NuGet.*" />
			<package pattern="dotnet-aspnet-codegenerator" />
			<package pattern="Microsoft.VisualStudio.Web.CodeGeneration.Design" />
			<package pattern="Microsoft.EntityFrameworkCore.Design" />
		</packageSource>
	</packageSourceMapping>
</configuration>
